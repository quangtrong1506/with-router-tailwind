{
    "name": "with-router-tailwind",
    "version": "1.0.0",
    "main": "expo-router/entry",
    "scripts": {
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "preview": "expo start --dev-client",
        "prebuild": "expo prebuild",
        "prebuild-clean": "expo prebuild --clean",
        "build-android": "pnpm prebuild && npx eas build -p android --profile production",
        "build-ios": "pnpm prebuild && npx eas build -p ios --profile production",
        "deploy": "npx expo export -p web && npx eas-cli@latest deploy"
    },
    "dependencies": {
        "@react-native-firebase/app": "^21.14.0",
        "@react-native-firebase/auth": "^21.14.0",
        "@react-native-firebase/messaging": "^21.14.0",
        "expo": "~52.0.27",
        "expo-constants": "~17.0.3",
        "expo-linking": "~7.0.3",
        "expo-notifications": "~0.29.14",
        "expo-router": "~4.0.11",
        "expo-splash-screen": "~0.29.13",
        "expo-status-bar": "~2.0.0",
        "firebase-admin": "^13.3.0",
        "nativewind": "^4.0.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.6",
        "react-native-reanimated": "~3.16.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.4.0",
        "react-native-web": "~0.19.10",
        "tailwindcss": "^3.4.0"
    },
    "devDependencies": {
        "@react-native-community/cli": "latest",
        "@babel/core": "^7.20.0",
        "@types/react": "~18.3.12",
        "typescript": "~5.3.3"
    }
}
